[Unit]
Description=Rundeck service
Wants=network-online.target
After=network-online.target

[Service]
User=rundeck
Group=rundeck
Type=simple
Restart=always
ExecStart=/bin/bash -c ". /etc/rundeck/profile ; /usr/bin/$rundeckd" | /usr/bin/tee /var/log/rundeck/service.log 2>&1"
LimitNOFILE=1048576
TimeoutStopSec=20
UMask=0027

[Install]
WantedBy=multi-user.target
